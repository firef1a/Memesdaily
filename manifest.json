{
  "manifest_version": 2,

  "name": "Memes daily",
  "description": "This extension lets you look at new memes daily!",
  "version": "1.0",
  "icons": { "128": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Memes daily"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}